{
	"expo": {
		"name": "Dropanidea",
		"slug": "dropanidea",
		"version": "0.1.0",
		"privacy": "public",
		"owner": "allister",
		"description": "Drop an idea, we discuss together.",
		"primaryColor": "#54A7E2",
		"orientation": "portrait",
		"icon": "./assets/icon.png",
		"splash": {
			"image": "./assets/splash.png",
			"resizeMode": "contain",
			"backgroundColor": "#ffffff"
		},
		"updates": {
			"fallbackToCacheTimeout": 0
		},
		"assetBundlePatterns": ["**/*"],
		"ios": {
			"supportsTablet": true
		},
		"android": {
			"package": "com.dropanidea.app",
			"versionCode": 1,
			"permissions": ["NOTIFICATIONS"],
			"adaptiveIcon": {
				"foregroundImage": "./assets/adaptive-icon.png",
				"backgroundColor": "#FFFFFF"
			}
		},
		"web": {
			"favicon": "./assets/favicon.png"
		}
	}
}
